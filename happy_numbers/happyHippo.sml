		fun parse file =
			let
				(* a function to read an integer from an input stream *)
				fun next_int input =
					Option.valOf (TextIO.scanStream (Int.scan StringCvt.DEC) input)
				(* open input file and read the two integers in the first line *)
				val stream = TextIO.openIn file
				val down = next_int stream
				val up = next_int stream
				val _ = TextIO.inputLine stream
				(* a function to read the pair of integer & real in subsequent lines *)
			in
				(down, up)
			end

		fun sum_of_digits 0 sum = sum
			|	sum_of_digits num sum =
			let
				val digit = num mod 10
			in
				sum_of_digits (num div 10) (sum + (digit * digit))
			end

		fun make_sums left right acc =
			if right = left then
				rev acc
			else
				make_sums (left + 1) right ((sum_of_digits left 0)::acc)

		fun my_solution (down, up) sums =
