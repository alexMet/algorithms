										else
											sum
								in
									mono_loop (from + 1) to sure newSum
								end
					in
						if dl = ul then
							mono_loop dr (ur + 1) dl 0
						else
							let
								fun duo_loop from to sum =
									if from = to then sum
									else
										let
											val newSum = mono_loop 0 100000 from sum
										in
											duo_loop (from + 1) to newSum
										end

								val sum1 = mono_loop dr 100000 dl 0
								val sum2 = duo_loop (dl + 1) ul sum1
								val sum3 = mono_loop 0 (ur + 1) ul sum2
							in
								sum3
							end
					end
			in
				find_sum (down div 100000) (down mod 100000) (up div 100000) (up mod 100000)
			end
